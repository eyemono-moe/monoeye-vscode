      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#C688F2"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FF66CC"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FF8566"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#C688F2"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.raw"],
      "settings": {
        "foreground": "#C688F2"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A2DB57"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#75F0DB"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#B2ABBA"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#8F98FF"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#B2ABBA"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F2EEF6"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FF66CC"
